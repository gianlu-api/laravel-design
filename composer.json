{
  "name": "gianlu-api/laravel-design",
  "description": "Library to generate custom design structure in laravel",
  "type": "library",
  "version": "0.6.1",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "gianluApi\\laravelDesign\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GianluApi\\LaravelDesign\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Gianluca Apicella",
      "email": "gianluca.api96@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/console": "^11 || ^12",
    "illuminate/support": "^11 || ^12"
  },
  "require-dev": {
    "pestphp/pest": "^2.0 || ^3.0",
    "larastan/larastan": "^3.0",
    "orchestra/testbench": "^9.0 || ^10"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "gianluApi\\laravelDesign\\Providers\\LaravelDesignServiceProvider"
      ]
    }
  },
  "keywords": [
    "PHP",
    "php",
    "laravel",
    "Laravel"
  ]
}
